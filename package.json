  },
  "dependencies": {
    "@patriarche/stylelint-config-base": "^1.1.0",
    "postcss-html": "^1.0.0",
    "stylelint": "14.16.1",
    "stylelint-config-standard-vue": "1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2"
  },
  "peerDependencies": {
    "stylelint": "14.16.1"
  }
}